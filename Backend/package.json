{
  "name": "pbl6-api",
  "version": "0.15.1",
  "licenses": [
    {
      "type": "UNLICENSED"
    }
  ],
  "description": "e+ SDK API - EMPS",
  "main": "app.local.js",
  "config": {
    "s3BucketName": "YOUR_UNIQUE_BUCKET_NAME",
    "region": "YOUR_AWS_REGION",
    "cloudFormationStackName": "AwsServerlessExpressStack",
    "functionName": "YOUR_SERVERLESS_EXPRESS_LAMBDA_FUNCTION_NAME",
    "accountId": "YOUR_ACCOUNT_ID"
  },
  "scripts": {
    "start": "nodemon --ignore tmp/ --inspect=0.0.0.0 app.local.js",
    "debug": "nodemon --ignore tmp/ --inspect=0.0.0.0:9230 app.local.js",
    "migration:generate": "typeorm migration:generate -d src/database/ormconfig.js -o src/database/migrations/$npm_config_name",
    "migration:run": "typeorm migration:run -d src/database/ormconfig.js",
    "migration:revert": "typeorm migration:revert -d src/database/ormconfig.js",
    "seed:run": "node src/database/seeds/seed-run.js",
    "lint": "eslint \"**/*.js\" --ignore-pattern node_modules/",
    "test": "NODE_ENV=test jest --runInBand",
    "prepare": "husky install"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "eslint": "8.28.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-chai-friendly": "0.7.2",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.2",
    "jest": "29.3.1",
    "nodemon": "2.0.20",
    "sequelize-cli": "6.5.2",
    "sqlite3": "^5.1.4",
    "superagent": "8.0.3",
    "supertest": "6.3.1"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@sentry/node": "^7.37.2",
    "@slack/webhook": "6.1.0",
    "async": "3.1.0",
    "base32.js": "^0.1.0",
    "bcrypt": "^5.1.1",
    "blob-polyfill": "^9.0.20240710",
    "body-parser": "1.17.1",
    "bull": "^4.16.3",
    "cloudinary": "^2.4.0",
    "cls-hooked": "^4.2.2",
    "compression": "1.6.2",
    "cookie-parser": "^1.4.6",
    "cors": "2.8.3",
    "dotenv": "16.0.3",
    "express": "4.15.2",
    "http-proxy-middleware": "^2.0.6",
    "jsonwebtoken": "^9.0.2",
    "lodash": "4.17.15",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "mysql2": "^3.11.0",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.9.15",
    "otpauth": "^9.3.4",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.2",
    "speakeasy": "^2.0.0",
    "typeorm": "^0.3.20",
    "winston": "3.2.1",
    "winston-daily-rotate-file": "4.4.1"
  }
}
